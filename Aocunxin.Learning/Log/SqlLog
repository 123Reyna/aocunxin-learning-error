[OrderSort] int NOT NULL  ,
[Icon] nvarchar(100) NULL  ,
[Description] nvarchar(100) NULL  ,
[Enabled] bit NOT NULL  ,
[CreateId] int NULL  ,
[CreateBy] nvarchar(50) NULL  ,
[CreateTime] datetime NULL  ,
[ModifyId] int NULL  ,
[ModifyBy] nvarchar(50) NULL  ,
[ModifyTime] datetime NULL  ,
[IsDeleted] bit NULL  )
